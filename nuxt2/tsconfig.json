{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*" : ["types/*", "*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "@/pages/*": ["pages/*"],
      "@/styles/*": ["styles/*"]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/i18n",
      "@nuxtjs/sanity",
      "@types/node",
      "./generated/*"
    ]
  },
  "vueCompilerOptions": {
    "experimentalCompatMode": 2
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
