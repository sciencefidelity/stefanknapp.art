{
  "name": "stefanknapp",
  "description": "The art, life and estate of Polish enamelist Stefan Knapp",
  "private": true,
  "version": "0.1.0",
  "main": "package.json",
  "author": "Matt Cook <sciencefidelity@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:sciencefidelity/stefanknapp.git"
  },
  "keywords": [
    "sanity"
  ],
  "scripts": {
    "start": "sanity start",
    "dev": "sanity start",
    "build": "sanity build",
    "format": "prettier --write '**/*.{ts,js,tsx,jsx,json,md,yml}'"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "@sanity/base",
      "@sanity/core",
      "@sanity/dashboard",
      "@sanity/default-layout",
      "@sanity/default-login",
      "@sanity/desk-tool",
      "@sanity/document-internationalization",
      "@sanity/eslint-config-studio",
      "@sanity/language-filter",
      "@sanity/vision",
      "react",
      "react-dom"
    ]
  },
  "dependencies": {
    "@sanity/base": "^2.31.0",
    "@sanity/core": "^2.31.0",
    "@sanity/dashboard": "^2.31.0",
    "@sanity/default-layout": "^2.31.0",
    "@sanity/default-login": "^2.31.0",
    "@sanity/desk-tool": "^2.31.0",
    "@sanity/document-internationalization": "^0.3.5",
    "@sanity/language-filter": "^2.31.0",
    "@sanity/vision": "^2.31.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanity-plugin-dashboard-widget-vercel": "^1.1.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^2.0.0",
    "eslint": "^8.17.0",
    "prettier": "^2.6.2"
  }
}
